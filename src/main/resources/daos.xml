<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        https://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="authorDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.AuthorDaoImpl"
          autowire="byType"/>
    <bean id="bookAuthorDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.BookAuthorDaoImpl"
          autowire="byType"/>
    <bean id="bookCategoryDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.BookCategoryDaoImpl"
          autowire="byType"/>
    <bean id="bookDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.BookDaoImpl"
          autowire="byType"/>
    <bean id="categoryDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.CategoryDaoImpl"
          autowire="byType"/>
    <bean id="customerDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.CustomerDaoImpl"
          autowire="byType"/>
    <bean id="inventoryDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.InventoryDaoImpl"
          autowire="byType"/>
    <bean id="languageDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.LanguageDaoImpl"
          autowire="byType"/>
    <bean id="profileDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.ProfileDaoImpl"
          autowire="byType"/>
    <bean id="publisherDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.PublisherDaoImpl"
          autowire="byType"/>
    <bean id="rentalDetailDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.RentalDetailDaoImpl"
          autowire="byType"/>
    <bean id="rentalHeaderDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.RentalHeaderDaoImpl"
          autowire="byType"/>
    <bean id="returnDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.ReturnDaoImpl"
          autowire="byType"/>
    <bean id="roleDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.RoleDaoImpl"
          autowire="byType"/>
    <bean id="userDao"
          class="com.lawencon.bookleasing.dao.hibernate.hql.UserDaoImpl"
          autowire="byType"/>

</beans>