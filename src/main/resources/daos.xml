<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        https://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="authorDao"
          class="com.lawencon.bookleasing.dao.impl.AuthorDaoImpl"
          autowire="byType"/>
    <bean id="bookAuthorDao"
          class="com.lawencon.bookleasing.dao.impl.BookAuthorDaoImpl"
          autowire="byType"/>
    <bean id="bookCategoryDao"
          class="com.lawencon.bookleasing.dao.impl.BookCategoryDaoImpl"
          autowire="byType"/>
    <bean id="bookDao"
          class="com.lawencon.bookleasing.dao.impl.BookDaoImpl"
          autowire="byType"/>
    <bean id="categoryDao"
          class="com.lawencon.bookleasing.dao.impl.CategoryDaoImpl"
          autowire="byType"/>
    <bean id="customerDao"
          class="com.lawencon.bookleasing.dao.impl.CustomerDaoImpl"
          autowire="byType"/>
    <bean id="inventoryDao"
          class="com.lawencon.bookleasing.dao.impl.InventoryDaoImpl"
          autowire="byType"/>
    <bean id="languageDao"
          class="com.lawencon.bookleasing.dao.impl.LanguageDaoImpl"
          autowire="byType"/>
    <bean id="profileDao"
          class="com.lawencon.bookleasing.dao.impl.ProfileDaoImpl"
          autowire="byType"/>
    <bean id="publisherDao"
          class="com.lawencon.bookleasing.dao.impl.PublisherDaoImpl"
          autowire="byType"/>
    <bean id="rentalDetailDao"
          class="com.lawencon.bookleasing.dao.impl.RentalDetailDaoImpl"
          autowire="byType"/>
    <bean id="rentalHeaderDao"
          class="com.lawencon.bookleasing.dao.impl.RentalHeaderDaoImpl"
          autowire="byType"/>
    <bean id="returnDao"
          class="com.lawencon.bookleasing.dao.impl.ReturnDaoImpl"
          autowire="byType"/>
    <bean id="roleDao"
          class="com.lawencon.bookleasing.dao.impl.RoleDaoImpl"
          autowire="byType"/>
    <bean id="userDao"
          class="com.lawencon.bookleasing.dao.impl.UserDaoImpl"
          autowire="byType"/>

</beans>