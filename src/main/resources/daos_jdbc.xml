<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        https://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="authorDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.AuthorDaoJdbcImpl"
          autowire="byType"/>
    <bean id="bookAuthorDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.BookAuthorDaoJdbcImpl"
          autowire="byType"/>
    <bean id="bookCategoryDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.BookCategoryDaoJdbcImpl"
          autowire="byType"/>
    <bean id="bookDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.BookDaoJdbcImpl"
          autowire="byType"/>
    <bean id="categoryDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.CategoryDaoJdbcImpl"
          autowire="byType"/>
    <bean id="customerDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.CustomerDaoJdbcImpl"
          autowire="byType"/>
    <bean id="inventoryDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.InventoryDaoJdbcImpl"
          autowire="byType"/>
    <bean id="languageDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.LanguageDaoJdbcImpl"
          autowire="byType"/>
    <bean id="profileDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.ProfileDaoJdbcImpl"
          autowire="byType"/>
    <bean id="publisherDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.PublisherDaoJdbcImpl"
          autowire="byType"/>
    <bean id="rentalDetailDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.RentalDetailDaoJdbcImpl"
          autowire="byType"/>
    <bean id="rentalHeaderDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.RentalHeaderDaoJdbcImpl"
          autowire="byType"/>
    <bean id="returnDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.ReturnDaoJdbcImpl"
          autowire="byType"/>
    <bean id="roleDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.RoleDaoJdbcImpl"
          autowire="byType"/>
    <bean id="userDao"
          class="com.lawencon.bookleasing.dao.jdbcImpl.UserDaoJdbcImpl"
          autowire="byType"/>

</beans>