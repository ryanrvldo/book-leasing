<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        https://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="authorRepository"
          class="com.lawencon.bookleasing.repository.impl.AuthorRepositoryImpl"
          autowire="constructor"/>
    <bean id="bookAuthorRepository"
          class="com.lawencon.bookleasing.repository.impl.BookAuthorRepositoryImpl"
          autowire="constructor"/>
    <bean id="bookCategoryRepository"
          class="com.lawencon.bookleasing.repository.impl.BookCategoryRepositoryImpl"
          autowire="constructor"/>
    <bean id="bookRepository"
          class="com.lawencon.bookleasing.repository.impl.BookRepositoryImpl"
          autowire="constructor"/>
    <bean id="categoryRepository"
          class="com.lawencon.bookleasing.repository.impl.CategoryRepositoryImpl"
          autowire="constructor"/>
    <bean id="customerRepository"
          class="com.lawencon.bookleasing.repository.impl.CustomerRepositoryImpl"
          autowire="constructor"/>
    <bean id="inventoryRepository"
          class="com.lawencon.bookleasing.repository.impl.InventoryRepositoryImpl"
          autowire="constructor"/>
    <bean id="languageRepository"
          class="com.lawencon.bookleasing.repository.impl.LanguageRepositoryImpl"
          autowire="constructor"/>
    <bean id="profileRepository"
          class="com.lawencon.bookleasing.repository.impl.ProfileRepositoryImpl"
          autowire="constructor"/>
    <bean id="publisherRepository"
          class="com.lawencon.bookleasing.repository.impl.PublisherRepositoryImpl"
          autowire="constructor"/>
    <bean id="rentalDetailRepository"
          class="com.lawencon.bookleasing.repository.impl.RentalDetailRepositoryImpl"
          autowire="constructor"/>
    <bean id="rentalHeaderRepository"
          class="com.lawencon.bookleasing.repository.impl.RentalHeaderRepositoryImpl"
          autowire="constructor"/>
    <bean id="returnRepository"
          class="com.lawencon.bookleasing.repository.impl.ReturnRepositoryImpl"
          autowire="constructor"/>
    <bean id="roleRepository"
          class="com.lawencon.bookleasing.repository.impl.RoleRepositoryImpl"
          autowire="constructor"/>
    <bean id="userRepository"
          class="com.lawencon.bookleasing.repository.impl.UserRepositoryImpl"
          autowire="constructor"/>

</beans>