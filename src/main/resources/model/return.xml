<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.lawencon.bookleasing.entity.Return" table="tb_r_return">
        <id name="id">
            <generator class="identity"/>
        </id>
        <many-to-one name="rentalHeader" class="com.lawencon.bookleasing.entity.RentalHeader">
            <column name="rental_hdr_id" not-null="true"/>
        </many-to-one>
        <property name="returnedAt">
            <column name="return_date" not-null="true"/>
        </property>
        <many-to-one name="user" class="com.lawencon.bookleasing.entity.User">
            <column name="user_id" not-null="true"/>
        </many-to-one>
        <property name="totalPrice">
            <column name="total_price"/>
        </property>
    </class>

</hibernate-mapping>