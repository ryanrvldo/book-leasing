<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.lawencon.bookleasing.entity.Bookshelf" table="tb_m_bookshelf">
        <id name="id">
            <generator class="identity"/>
        </id>
        <property name="code" length="20"/>
        <property name="rowPosition">
            <column name="row_position" not-null="true"/>
        </property>
        <property name="columnPosition">
            <column name="column_position" not-null="true"/>
        </property>
    </class>

</hibernate-mapping>